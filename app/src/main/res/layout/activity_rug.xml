<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".RougActivity">

    <LinearLayout
        android:id="@+id/rog"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/colorSeekBar"
        android:layout_alignParentStart="true"
        android:orientation="vertical"
        android:weightSum="1">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight=".1"
            android:orientation="horizontal"
            android:id="@+id/row0">

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:layout_weight=".1"
                android:background="#ffffff"
                android:layout_margin="2dp"
                android:id="@+id/frame00"></FrameLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:layout_weight=".15"
                android:background="#ff9d52"
                android:layout_margin="2dp"
                android:id="@+id/frame01"></FrameLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:layout_weight=".2"
                android:background="#d65e0b"
                android:layout_margin="2dp"
                android:id="@+id/frame02"></FrameLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:layout_weight=".25"
                android:background="#802919"
                android:layout_margin="2dp"
                android:id="@+id/frame03"></FrameLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:layout_weight=".3"
                android:background="#aa9b4c"
                android:layout_margin="2dp"
                android:id="@+id/frame04"></FrameLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight=".15"
            android:orientation="horizontal"
            android:id="@+id/row1">

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:layout_weight=".1"
                android:background="#aa9b4c"
                android:layout_margin="2dp"
                android:id="@+id/frame10"
                android:clickable="true"></FrameLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:layout_weight=".15"
                android:background="#FFFFFF"
                android:layout_margin="2dp"
                android:id="@+id/frame11"></FrameLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:layout_weight=".2"
                android:background="#ff9d52"
                android:layout_margin="2dp"
                android:id="@+id/frame12"></FrameLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:layout_weight=".25"
                android:background="#d65e0b"
                android:layout_margin="2dp"
                android:id="@+id/frame13"></FrameLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:layout_weight=".3"
                android:background="#802919"
                android:layout_margin="2dp"
                android:id="@+id/frame14"></FrameLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight=".2"
            android:orientation="horizontal"
            android:id="@+id/row2">

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:layout_weight=".1"
                android:background="#802919"
                android:layout_margin="2dp"
                android:id="@+id/frame20"></FrameLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:layout_weight=".15"
                android:background="#aa9b4c"
                android:layout_margin="2dp"
                android:id="@+id/frame21"></FrameLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:layout_weight=".2"
                android:background="#ffffff"
                android:layout_margin="2dp"
                android:id="@+id/frame22"></FrameLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:layout_weight=".25"
                android:background="#ff9d52"
                android:layout_margin="2dp"
                android:id="@+id/frame23"></FrameLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:layout_weight=".3"
                android:background="#d65e0b"
                android:layout_margin="2dp"
                android:id="@+id/frame24"></FrameLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight=".25"
            android:orientation="horizontal"
            android:id="@+id/row3">

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:layout_weight=".1"
                android:background="#d65e0b"
                android:layout_margin="2dp"
                android:id="@+id/frame30"></FrameLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:layout_weight=".15"
                android:background="#802919"
                android:layout_margin="2dp"
                android:id="@+id/frame31"></FrameLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:layout_weight=".2"
                android:background="#aa9b4c"
                android:layout_margin="2dp"
                android:id="@+id/frame32"></FrameLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:layout_weight=".25"
                android:background="#ffffff"
                android:layout_margin="2dp"
                android:id="@+id/frame33"></FrameLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:layout_weight=".3"
                android:background="#ff9d52"
                android:layout_margin="2dp"
                android:id="@+id/frame34"></FrameLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight=".3"
            android:orientation="horizontal"
            android:id="@+id/row4">

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:layout_weight=".1"
                android:background="#ff9d52"
                android:layout_margin="2dp"
                android:id="@+id/frame40"></FrameLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:layout_weight=".15"
                android:background="#d65e0b"
                android:layout_margin="2dp"
                android:id="@+id/frame41"></FrameLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:layout_weight=".2"
                android:background="#802919"
                android:layout_margin="2dp"
                android:id="@+id/frame42"></FrameLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:layout_weight=".25"
                android:background="#aa9b4c"
                android:layout_margin="2dp"
                android:id="@+id/frame43"></FrameLayout>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:layout_weight=".3"
                android:background="#ffffff"
                android:layout_margin="2dp"
                android:id="@+id/frame44"></FrameLayout>
        </LinearLayout>
    </LinearLayout>

    <SeekBar
        android:id="@+id/colorSeekBar"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/rog" />
</RelativeLayout>
